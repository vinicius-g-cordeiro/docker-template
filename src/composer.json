{
    "name": "vinicius-g-cordeiro/docker-template",
    "description": "Simple Docker template for development using Nginx, PHP 8.4 FPM, SQL Database: MySQL, No-SQL Databases: MongoDB, NodeJS, Composer",
    "type": "project",
    "readme": "README.md",
    "require": {
        "php": "^8.3",
        "adodb/adodb": "^2.2",
        "guzzlehttp/guzzle": "^7.2",
        "mongodb/mongodb": "^1.13",
        "twig/twig": "^3.4",
        "cboden/ratchet": "^0.4.4",
        "phpmailer/phpmailer": "^6.9",
		"tecnickcom/tcpdf": "^6.9"
    },
    "require-dev" : {
        "phpunit/phpunit" : "^10.0"
    },
    "autoload" : {
        "psr-4" : {
			"App\\": "app/"
        },
        "classmap": [            
        ]
    },
    "config": {
        "vendor-dir": "libs/"
    }
}