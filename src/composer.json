{
    "name": "vinicius-g-cordeiro/docker-template",
    "description": "Simple Docker template for development using Nginx, PHP 8.4 FPM, SQL Database: MySQL, No-SQL Databases: MongoDB, NodeJS, Composer",
    "type": "project",
    "readme": "README.md",
    "require": {
        "php": "^8.4",
        "guzzlehttp/guzzle": "^7.9",
        "adodb/adodb-php": "^5.22",
        "phpmailer/phpmailer": "^6.9",
		"tecnickcom/tcpdf": "^6.9",
		"symfony/http-foundation": "^6.3",
		"symfony/http-kernel": "^6.3",
		"symfony/routing": "^6.3",
		"symfony/translation": "^6.3",
		"symfony/validator": "^6.3",
		"symfony/yaml": "^6.3",
		"symfony/console": "^6.3",
		"symfony/finder": "^6.3",
		"symfony/filesystem": "^6.3",
		"twig/twig": "^3.0",
		"cboden/ratchet": "^0.4.4"
    },
    "require-dev" : {
        "phpunit/phpunit" : "^10.0"
    },
    "autoload" : {
        "psr-4" : {
			"App\\": "app/",
            "Config\\": "config/"
        },
        "classmap": [
            "config/"
        ]
    },
    "config": {
        "vendor-dir": "libs/"
    }
}