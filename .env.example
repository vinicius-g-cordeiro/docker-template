# .env file for Docker Compose configuration
# This file contains environment variables for the MySQL and MongoDB services.

# MySQL Configuration
MYSQL_VERSION=8.0
MYSQL_HOST=your_mysql_host
MYSQL_ROOT_USER=your_mysql_root_user
MYSQL_ROOT_PASSWORD=your_mysql_root_password
MYSQL_DATABASE=your_application_sql_database
MYSQL_USER=your_application_sql_user
MYSQL_PASSWORD=your_application_sql_password

# MongoDB Configuration
MONGO_VERSION=latest
MONGO_INITDB_ROOT_USERNAME=your_mongodb_root_username
MONGO_INITDB_ROOT_PASSWORD=your_mongodb_root_password
MONGO_INITDB_DATABASE=your_application_mongodb_database

# Node.js Configuration
NODE_VERSION=alpine
NODE_ENV=development

# Application Configuration
APP_NAME=your_application_name
APP_PORT=3000
APP_HOST=localhost
APP_DEBUG=true
APP_KEY=your_application_key